<odoo>
    <data>

        <record id="view_chamcong_list" model="ir.ui.view">
            <field name="name">chamcong</field>
            <field name="model">chamcong</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="user_id"  width="150"/>
                    <field name="product_id" width="150"/>
                    <field name="quantity" width="150"/>
                    <field name="datetime" width="150"/>
                </tree>
            </field>
        </record>

        <record id="view_chamcong_list_addmore" model="ir.ui.view">
            <field name="name">Timekeeping for more</field>
            <field name="model">chamcong</field>
            <field name="arch" type="xml">
                <tree editable='bottom'>
                    <field name="user_id"  width="180" />
                    <field name="product_id" width="180" />
                    <field name="quantity"  width="180" />
                    <field name="datetime"  width="180" />
                </tree>
            </field>
        </record>


        <record id="view_form_timekeeping" model="ir.ui.view">
            <field name="name">Timekeeping</field>
            <field name="model">chamcong</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group name="group_top">
                            <group name="group_left">
                                <field name="user_id" />
                                <field name="product_id" />
                            </group>
                            <group name="group_right">
                                <field name="quantity" />
                                <field name="datetime" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>


