<odoo>
    <!-- Action to open the Book list -->
    <record id="action_timekeeping_table" model="ir.actions.act_window">
        <field name="name">Timekeeping Table</field>
        <!-- <field name="view_id" ref="timekeeping_app.view_tree_timekeeping"/> -->
        <field name="res_model">timekeeping.table</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="action_timekeeping_table2" model="ir.actions.act_window">
        <field name="name">List</field>
        <!-- <field name="view_id" ref="timekeeping_app.view_tree_timekeeping_edit"/> -->
        <field name="res_model">timekeeping.table</field>
        <field name="view_mode">tree</field>
    </record>
    <!-- Timekeeping App Menu -->
    <!-- <menuitem id="menu_timekeeping" name="Timekeeping" /> -->
    <!-- Menu item to open the Book list -->
    <menuitem id="menu_timekeeping_table"
        name="Timekeeping"
        action="action_timekeeping_table"
    />
    <menuitem id="menu_timekeeping_test"
        name="List View"
        parent="menu_timekeeping_table"
        action="action_timekeeping_table2"
    />
</odoo>